<rn:block id="topChannelAttribute"/>
<div class="<%= className %>">
    <% if (contentAttribute.type && contentAttribute.type === 'FILE' ) { %>
        <div class='rn_SchemaAttribute'><%= contentAttribute.name %></div>
        <div class='rn_SchemaAttributeValue'><a href="/ci/okcsFattach/get/<%= answerID %>_<%= contentAttribute.position %>"><%= contentAttribute.value %></a></div>
    <% } else if (contentAttribute.type && contentAttribute.type === 'CHECKBOX') {
        var checkboxID = className + index,
            checked = contentAttribute.value === 'Y'; %>
        <div class='rn_SchemaAttribute'>
            <label for="<%= checkboxID %>"><%= contentAttribute.name %></label>
            <input id="<%= checkboxID %>" type="checkbox" disabled <%=checked ? "checked" : "" %>>
        </div>
        <div class='rn_SchemaAttributeValue'></div>
    <% } else { %>
        <div class='rn_SchemaAttribute'><%= contentAttribute.name %></div>
        <div class='rn_SchemaAttributeValue'><%= contentAttribute.value %></div>
    <% } %>
</div>